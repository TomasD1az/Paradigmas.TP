package stack;

import java.util.ArrayList;

public class ListWithItems extends SuperList {
	public ArrayList<Object> array;
	
	public ListWithItems( Object object) {
		array = new ArrayList<Object>();
		array.add( object );
	}
	
	public boolean isEmpty() {
		return false;
	}
	
	public SuperList push( Object object) {
		array.add( object );
		return SuperList.ListWith( array );
	}
	
	public SuperList pop() {
		array.remove( array.size() - 1 );
		return SuperList.ListWith( array );
	}
	
	public Object top() {
		return array.get( size() - 1);
	}
	
	public Integer size() {
		return array.size();
	}

}
