package stack;

import java.util.ArrayList;

	public abstract class SuperList {
		
		static public SuperList ListWith(ArrayList<Object> array) {
			SuperList returnList = new EmptyList();
			for (int i = 0; i < array.size(); i++) 
				returnList.push(array.get(i));
			
			return returnList;
	}
		
		public abstract boolean isEmpty();
		public abstract SuperList push( Object object);
		public abstract SuperList pop();
		public abstract Object top();
		public abstract Integer size();
}
